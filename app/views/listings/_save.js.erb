$("ul.errors").html("")
<% if @listing.errors.any? %>
  <% @listing.errors.full_messages.each do |message| %>
    $("ul.errors").append($("<li />").html("<%= message.html_safe %>"))
  <% end %>
<% else %>
  $(".listing-index").html("<%= escape_javascript(render 'index') %>")
  $("#listing-modal").modal("hide")
<% end %>